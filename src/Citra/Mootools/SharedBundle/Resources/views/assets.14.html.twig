{% block mootools %}

    {% javascripts
        combine=true
        output='js/mootools/core.js'
        'bundles/mootools/js/mootools-core-1.4.5.js'
    %}<script type="text/javascript" src="{{
        asset_url
    }}"></script>{% endjavascripts %}

    {% javascripts
        combine=true
